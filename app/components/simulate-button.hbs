    {{#each this.roundMatchups as |matchup|}}
        {{#each matchup as |team teamIndex|}}
            {{team.seed}} {{team.teamName}}
            {{#if (not-eq teamIndex 1)}}
                vs
            {{/if}}
        {{/each}}
        <br/>
    {{/each}}
<button type="button" {{action this.simulateRound this.round}}>
    Simulate {{this.roundName}}
</button>
{{#if this.isOver}}
    <br/>
    {{this.tournamentWinner.teamName}} wins!!!!
{{/if}}